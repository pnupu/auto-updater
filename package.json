{
  "name": "devpost-autoupgrader",
  "version": "1.0.0",
  "description": "AI-powered dependency upgrade tool that automatically handles breaking changes",
  "type": "module",
  "main": "./dist/index.js",
  "bin": {
    "devpost-upgrade": "./dist/cli.js"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "type-check": "tsc --noEmit",
    "lint": "eslint src",
    "start": "node dist/cli.js"
  },
  "keywords": [
    "dependency",
    "upgrade",
    "ai",
    "gemini",
    "automation",
    "npm",
    "migration"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@babel/parser": "^7.26.3",
    "@babel/traverse": "^7.26.5",
    "@google/generative-ai": "^0.21.0",
    "@langchain/langgraph": "^0.2.19",
    "@langchain/google-genai": "^0.1.4",
    "commander": "^12.1.0",
    "chalk": "^5.3.0",
    "ora": "^8.1.1",
    "execa": "^9.5.2",
    "simple-git": "^3.27.0",
    "npm-check-updates": "^17.1.14",
    "package-json": "^10.0.1",
    "semver": "^7.6.3",
    "fast-glob": "^3.3.2",
    "ignore": "^6.0.2",
    "axios": "^1.7.9",
    "cheerio": "^1.0.0",
    "cli-table3": "^0.6.5",
    "dotenv": "^16.4.7"
  },
  "devDependencies": {
    "@types/babel__traverse": "^7.20.6",
    "@types/node": "^22.10.2",
    "@types/semver": "^7.5.8",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2",
    "vitest": "^2.1.8",
    "eslint": "^9.17.0",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@typescript-eslint/parser": "^8.18.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
